{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "error_page": "index.html",
  "clean_urls": false
}